        android:layout_marginLeft="8dp"
        android:layout_marginTop="28dp"
        android:background="#03A9F4"
        android:gravity="center"
        android:text="@string/cell_view10"
        app:layout_constraintStart_toEndOf="@+id/element_9"
        app:layout_constraintTop_toBottomOf="@+id/element_5" />

    <TextView
        android:id="@+id/element_11"
        android:layout_width="78dp"
        android:layout_height="43dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="28dp"
        android:background="#03A9F4"
        android:gravity="center"
        android:text="@string/cell_view11"
