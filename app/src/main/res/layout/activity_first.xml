<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FirstActivity">

    <Button
        android:id="@+id/push_button"
        android:layout_width="65dp"
        android:layout_height="43dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="16dp"
        android:gravity="center"
        android:onClick="push_stack"
        android:text="@string/push_button"
        app:layout_constraintStart_toEndOf="@+id/element_insert_typed"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="OnClick" />

    <EditText
        android:id="@+id/element_insert_typed"
        android:layout_width="88dp"
        android:layout_height="43dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="16dp"
        android:autofillHints=""
        android:ems="10"
        android:gravity="center"
        android:hint="@string/insert_string"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toStartOf="@+id/push_button"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/element_1"
        android:layout_width="78dp"
        android:layout_height="43dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="@android:dimen/notification_large_icon_height"
        android:background="#03A9F4"
        android:gravity="center"
        android:text="@string/cell_view1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/element_insert_typed" />

    <TextView
        android:id="@+id/element_2"
        android:layout_width="78dp"
        android:layout_height="43dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="64dp"
        android:background="#03A9F4"
        android:gravity="center"
        android:text="@string/cell_view2"
        app:layout_constraintStart_toEndOf="@+id/element_1"
        app:layout_constraintTop_toBottomOf="@+id/push_button" />

    <Button
        android:id="@+id/pop_button"
        android:layout_width="65dp"
        android:layout_height="43dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="16dp"
        android:onClick="pop_stack"
        android:text="@string/pop_button"
        app:layout_constraintStart_toEndOf="@+id/push_button"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/element_3"
        android:layout_width="78dp"
        android:layout_height="43dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="64dp"
        android:background="#03A9F4"
        android:gravity="center"
        android:text="@string/cell_view3"
        app:layout_constraintStart_toEndOf="@+id/element_2"
        app:layout_constraintTop_toBottomOf="@+id/element_insert_typed" />

    <TextView
        android:id="@+id/element_4"
        android:layout_width="78dp"
        android:layout_height="43dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="64dp"
        android:background="#03A9F4"
        android:gravity="center"
        android:text="@string/cell_view4"
        app:layout_constraintStart_toEndOf="@+id/element_3"
        app:layout_constraintTop_toBottomOf="@+id/element_insert_typed" />


    <TextView
        android:id="@+id/element_5"
        android:layout_width="78dp"
