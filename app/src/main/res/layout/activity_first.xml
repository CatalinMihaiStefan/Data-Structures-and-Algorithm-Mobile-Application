        android:gravity="center"
        android:text="@string/cell_view7"
        app:layout_constraintStart_toEndOf="@+id/element_6"
        app:layout_constraintTop_toBottomOf="@+id/element_1" />

    <TextView
        android:id="@+id/element_8"
